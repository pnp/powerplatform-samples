      "updatedUserId": "2d6cf2d7-fad0-4185-94ca-3f66e090f31f"
    },
    {
      "channelContent": {
        "web": {
          "contentFormat": "Markdown",
          "content": "We have the product for you!\n\nThe Contoso Laptop i3 has enough power and memory to last you years."
        }
      },
      "id": "f39e45e5-6315-47fe-b425-0feacf2d3292",
